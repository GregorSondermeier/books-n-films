{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "originalTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "internationalTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "synopsis": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::person.person",
      "inversedBy": "writtenBooks"
    },
    "countries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "mappedBy": "books"
    },
    "year": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "maxLength": 4,
      "minLength": 4,
      "regex": "^[0-9]{4}$"
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "books"
    },
    "alternativeTitles": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "text"
    },
    "seriess": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::book-series.book-series",
      "inversedBy": "books"
    },
    "versions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::book-version.book-version",
      "mappedBy": "book"
    }
  }
}
