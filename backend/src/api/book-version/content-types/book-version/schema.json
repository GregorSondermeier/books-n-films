{
  "kind": "collectionType",
  "collectionName": "book_versions",
  "info": {
    "singularName": "book-version",
    "pluralName": "book-versions",
    "displayName": "BookVersions",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "medium": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "hardcover",
        "paperback",
        "ebook",
        "audiobook",
        "other"
      ]
    },
    "Publisher": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "bookVersions"
    },
    "notes": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    }
  }
}
