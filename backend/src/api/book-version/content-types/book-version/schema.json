{
  "kind": "collectionType",
  "collectionName": "book_versions",
  "info": {
    "singularName": "book-version",
    "pluralName": "book-versions",
    "displayName": "BookVersion",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "medium": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "hardcover",
        "paperback",
        "ebook",
        "audiobook",
        "other",
        "unknown"
      ],
      "required": true,
      "default": "unknown"
    },
    "publisher": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "bookVersions"
    },
    "notes": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "bookSet": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::book-set.book-set",
      "inversedBy": "bookVersions"
    },
    "book": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::book.book",
      "inversedBy": "versions"
    }
  }
}
