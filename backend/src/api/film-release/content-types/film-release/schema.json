{
  "kind": "collectionType",
  "collectionName": "film_releases",
  "info": {
    "singularName": "film-release",
    "pluralName": "film-releases",
    "displayName": "FilmRelease",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "medium": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "uhdbluray",
        "bluray",
        "hddvd",
        "dvd",
        "tv",
        "vod",
        "ld",
        "vcd",
        "vhs",
        "other",
        "unknown"
      ],
      "default": "unknown",
      "required": true
    },
    "label": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "filmReleases"
    },
    "notes": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    },
    "packaging": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "enumeration",
      "enum": [
        "softcase",
        "digipack",
        "hardbox",
        "insert",
        "metalpak",
        "mediabook",
        "steelbook",
        "tinbox",
        "other",
        "unknown"
      ],
      "default": "unknown",
      "required": true
    },
    "edition": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "releaseCollection": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::release-collection.release-collection",
      "inversedBy": "filmReleases"
    },
    "film": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::film.film",
      "inversedBy": "releases"
    },
    "rating": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::rating.rating",
      "inversedBy": "filmReleases"
    }
  }
}
