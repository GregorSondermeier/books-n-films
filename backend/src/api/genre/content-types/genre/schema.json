{
  "kind": "collectionType",
  "collectionName": "genres",
  "info": {
    "singularName": "genre",
    "pluralName": "genres",
    "displayName": "Genres"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "books": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::book.book",
      "mappedBy": "genres"
    },
    "films": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::film.film",
      "mappedBy": "genres"
    }
  }
}
